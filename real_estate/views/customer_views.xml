<!-- real_estate/views/customer_views.xml -->

<odoo>
    <data>
        <record id="view_customer_form" model="ir.ui.view">
            <field name="name">real.estate.customer.form</field>
            <field name="model">real.estate.customer</field>
            <field name="arch" type="xml">
                <form string="Real Estate Customer">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="email"/>
                            <field name="phone"/>
                            <field name="properties"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
